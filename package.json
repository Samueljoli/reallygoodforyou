{
  "name": "three-seed",
  "version": "0.3.1",
  "description": "Three.js starter project boilerplate bundled with webpack",
  "keywords": [
    "webgl",
    "boilerplate",
    "three.js",
    "es6",
    "webpack"
  ],
  "scripts": {
    "start": "serve -s build",
    "dev": "webpack-dev-server --mode development",
    "heroku-postbuild": "npm run prebuild; npm run build",
    "prebuild": "rimraf ./build && mkdir ./build",
    "build": "webpack --mode production",
    "push": "git push heroku master"
  },
  "dependencies": {
    "colladaloader2asmodule": "git+https://git@github.com/Samueljoli/ColladaLoader2asModule.git",
    "serve": "10.0.2",
    "three": "^0.91.0",
    "three-obj-loader": "^1.1.3",
    "webpack-cli": "^3.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.20.2",
    "webpack-dev-server": "^3.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
